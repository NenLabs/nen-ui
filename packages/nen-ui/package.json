{
  "name": "@nenlabs/nenui",
  "version": "0.0.0",
  "private": false,
  "description": "Collection of unstyled Vue3 components",
  "author": "Egwuchukwu Diala (https://github.com/egdiala)",
  "homepage": "https://ui.nenlabs.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/NenLabs/nen-ui.git"
  },
  "keywords": [
    "nen",
    "ui",
    "nenlabs",
    "nenui",
    "nen-ui"
  ],
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/nen-ui.umd.cjs",
